<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>error.do</servlet-name>
        <servlet-class>cc.openhome.view.Error</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>error.do</servlet-name>
        <url-pattern>/error.view</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>success.do</servlet-name>
        <servlet-class>cc.openhome.view.Success</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>success.do</servlet-name>
        <url-pattern>/success.view</url-pattern>
    </servlet-mapping>

    <servlet>

        <servlet-name>login.do</servlet-name>
        <servlet-class>cc.openhome.controller.Login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login.do</servlet-name>
        <url-pattern>/login.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>member.do</servlet-name>
        <servlet-class>cc.openhome.view.Member</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>member.do</servlet-name>
        <url-pattern>/member.view</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>register.do</servlet-name>
        <servlet-class>cc.openhome.controller.Register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>register.do</servlet-name>
        <url-pattern>/register.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>message.do</servlet-name>
        <servlet-class>cc.openhome.controller.Message</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>message.do</servlet-name>
        <url-pattern>/message.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>delete</servlet-name>
        <servlet-class>cc.openhome.controller.Delete</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>delete</servlet-name>
        <url-pattern>/delete.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>cc.openhome.controller.Logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout.do</url-pattern>
    </servlet-mapping>
</web-app>